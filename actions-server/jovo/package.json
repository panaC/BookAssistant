{
  "name": "jovo-sample-podcast-player",
  "version": "2.0.2",
  "description": "Sample Podcast Player App for Amazon Alexa and Google Assistant",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "dependencies": {
    "axios": "^0.18.0",
    "jovo-db-filedb": "^2.0.2",
    "jovo-framework": "^2.2.6",
    "jovo-platform-alexa": "^2.0.2",
    "jovo-platform-googleassistant": "^2.0.2",
    "jovo-plugin-debugger": "^2.0.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.5",
    "@types/socket.io-client": "^1.4.32",
    "gts": "^1.0.0",
    "gulp": "^4.0.0",
    "gulp-install": "^1.1.0",
    "gulp-run-command": "0.0.9",
    "gulp-zip": "^4.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.19.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.14.0",
    "typescript": "~3.4.0"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "tscw": "tsc --watch",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "bundle": "gulp --gulpfile node_modules/jovo-framework/gulpfile.js build-ts --cwd ./",
    "start": "cd ./dist && node index.js --webhook",
    "launch": "npm start -- --launch",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jovotech/jovo-sample-voice-app-nodejs.git"
  },
  "author": "Jovo",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jovotech/jovo-sample-voice-app-nodejs/issues"
  },
  "homepage": "https://github.com/jovotech/jovo-sample-voice-app-nodejs#readme"
}
