{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "target": "es2017",
    "resolveJsonModule": true,
    "lib": [
      "es2018"
    ]
  },
  "include": [
    "src/*.ts",
    "src/**/*.ts",
    "test/*.ts",
    "test/**/*.ts",
    "typings/**/*.d.ts",
    "src/**/*.json"
  ]
}
